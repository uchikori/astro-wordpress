---
import type { HTMLTag } from "astro/types";
type Props = {
  styles: string;
  classes: string;
  level: 1 | 2 | 3 | 4 | 5 | 6;
  content: string;
};
const { level, styles, classes, content } = Astro.props;
const Element: HTMLTag = `h${level}`;
---

<Element set:html={content} class={classes} style={styles} />
